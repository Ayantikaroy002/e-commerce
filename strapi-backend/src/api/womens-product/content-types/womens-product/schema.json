{
  "kind": "collectionType",
  "collectionName": "womens_products",
  "info": {
    "singularName": "womens-product",
    "pluralName": "womens-products",
    "displayName": "Womens Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Product_name": {
      "type": "string",
      "required": true
    },
    "Product_description": {
      "type": "blocks"
    },
    "Product_image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Product_price": {
      "type": "decimal",
      "required": true
    },
    "Category": {
      "type": "enumeration",
      "enum": [
        "T-shirts",
        "Tops",
        "Dresses",
        "Jackets",
        "Sweatshirts"
      ],
      "required": true
    },
    "Color": {
      "type": "enumeration",
      "enum": [
        "Pink",
        "Blue",
        "Yellow",
        "Red",
        "Green",
        "White",
        "Black",
        "Grey",
        "Magenta",
        "Purple"
      ]
    },
    "AvailableQty": {
      "type": "integer",
      "required": true
    },
    "Size": {
      "type": "string",
      "required": true
    }
  }
}
