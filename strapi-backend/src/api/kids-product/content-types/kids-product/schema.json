{
  "kind": "collectionType",
  "collectionName": "kids_products",
  "info": {
    "singularName": "kids-product",
    "pluralName": "kids-products",
    "displayName": "Kids Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Product_name": {
      "type": "string",
      "required": true,
      "maxLength": 71,
      "minLength": 3
    },
    "Product_description": {
      "type": "blocks"
    },
    "Product_image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Product_price": {
      "required": true,
      "type": "decimal"
    },
    "Category": {
      "type": "enumeration",
      "enum": [
        "Shirts",
        "T-shirts",
        "Frocks",
        "Hoodies",
        "Tops"
      ],
      "required": true
    },
    "Color": {
      "type": "enumeration",
      "enum": [
        "Blue",
        "Yellow",
        "Pink",
        "Purple",
        "Red",
        "Green",
        "White",
        "Black",
        "Grey",
        "Magenda"
      ],
      "required": true
    },
    "AvailableQty": {
      "type": "integer"
    },
    "Size": {
      "type": "string",
      "required": true
    }
  }
}
